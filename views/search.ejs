<div class="author">
  <h1 class="author-heading">Search Results</h1>
</div>

<section class="articles">
  <ul class="article-ul">
    <% data.forEach(post=> { %>
      <li>
        <a href='/post/<%= post._id %>'>

          <span>
            <%= post.title %>
          </span>
          <span class="aritcle-list-date">
            <%= post.createdAt %>
          </span>
        </a>
      </li>
      <% }) %>
  </ul>
</section>